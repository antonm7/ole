{
  "expo": {
    "name": "olle",
    "slug": "olle",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#05004C"
    },
    "version": "1.0.0",
    "scheme": "olle",
    "orientation": "portrait",
    "icon": "./assets/images/olle2.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "bundleIdentifier": "com.migolkoanton.olle",
      "buildNumber": "1.0.6",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleLocalizations": [
          "he"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "ExpoLocalization_supportsRTL": true,
        "ExpoLocalization_forcesRTL": true
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/olle2.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "backgroundColor": "#05004C",
        "resizeMode": "contain",
        "image": "./assets/images/slpash.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/olle2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-localization",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "supportsRTL": true,
      "forcesRTL": true,
      "plugins": [
        "expo-localization"
      ],
      "eas": {
        "projectId": "9cb1482a-cbc7-45dc-bba1-52f60d64537f"
      }
    },
    "owner": "migolkoanton"
  }
}
